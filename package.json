{
  "name": "@ngx-datacontext/core",
  "description": "Data context service for Angular applications to easily create data services and link them to entities.",
  "version": "0.0.6",
  "homepage": "https://github.com/alfredoperez/ngx-datacontext",
  "author": "Alfredo Perez (https://github.com/alfredoperez)",
  "license": "MIT",
  "keywords": [
    "ng",
    "angular",
    "library.datacontext",
    "entity"
  ],
  "bugs": {
    "url": "https://github.com/alfredoperez/ngx-datacontext/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:doc": "compodoc -p libs/core/tsconfig.lib.json --theme Readthedocs -d dist/apps/documentation/compodoc ",
    "serve:doc": "compodoc -p libs/core/tsconfig.lib.json --theme Readthedocs -d dist/apps/documentation/compodoc -s",
    "copy-readme": "copy .\\README.md .\\dist\\libs\\core",
    "copy-license": "copy .\\LICENSE .\\dist\\libs\\core",
    "copy-files": "run-s copy-readme copy-license",
    "build:lib-pack": "ng build core && npm run copy-files &&  cd dist/libs/core && npm pack",
    "bump-version:major": "npm version major && cd libs/core && npm version major && git add package.json",
    "bump-version:minor": "npm version minor && cd libs/core && npm version minor && git add package.json",
    "bump-version:patch": "npm version patch && cd libs/core && npm version patch && git add package.json",
    "update-changelog:major": "changelog -M && git add CHANGELOG.md",
    "update-changelog:minor": "changelog -m && git add CHANGELOG.md",
    "update-changelog:patch": "changelog -p && git add CHANGELOG.md",
    "commit-git": "git commit -m \"build: adds new release\" && git push origin && git push origin --tags",
    "pre-publish:minor": "run-s update-changelog:minor bump-version:minor  build:lib-pack commit-git",
    "pre-publish:major": "run-s update-changelog:major bump-version:major build:lib-pack commit-git",
    "pre-publish:patch": "run-s update-changelog:patch bump-version:patch build:lib-pack commit-git",
    "affected": "nx affected"
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/fire": "^5.1.0",
    "@angular/forms": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@compodoc/compodoc": "^1.1.6",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@nrwl/nx": "7.1.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "faker": "^4.1.0",
    "firebase": "^5.5.7",
    "font-awesome": "^4.7.0",
    "include-media": "^1.4.9",
    "lodash": "^4.17.11",
    "ngx-highlightjs": "^2.1.4",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "7.0.1",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/builders": "7.1.1",
    "@nrwl/schematics": "7.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.0.0",
    "@types/jquery": "3.3.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.0",
    "generate-changelog": "^1.7.1",
    "husky": "^1.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.0.0",
    "jest-preset-angular": "6.0.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.15.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "main": "./dist/libs/core/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfredoperez/ngx-datacontext.git"
  }
}
